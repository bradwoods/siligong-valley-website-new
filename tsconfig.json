{
    "compilerOptions": {
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "allowJs": true,
        "checkJs": false,
        "jsx": "react",
        "moduleResolution": "node",
        "noImplicitThis": true,
        "lib": [
            "es2015",
            "dom"
        ],
        "module": "commonjs",
        "target": "es6",
    },
    "exclude": [
        "node_modules",
    ],
    "include": [
        "src",
        "content",
    ]
}